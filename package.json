{
  "name": "universe",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "jetify",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:debug": "cd android && ./gradlew assembleDebug",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "eslint": "node node_modules/eslint/bin/eslint.js src",
    "prettier": "prettier --write \"./**/*.{js,jsx,json}\""
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^3.0.3",
    "@react-native-community/async-storage": "^1.5.1",
    "axios": "^0.19.0",
    "color": "^3.1.2",
    "crypto-js": "^3.1.9-1",
    "lodash": "^4.17.15",
    "npm": "^6.10.1",
    "react": "16.8.3",
    "react-native": "0.59.8",
    "react-native-action-sheet": "^2.2.0",
    "react-native-actionsheet": "^2.4.2",
    "react-native-autogrow-textinput": "^5.2.0",
    "react-native-btr": "^1.1.4",
    "react-native-cached-image": "^1.4.3",
    "react-native-check-box": "^2.1.7",
    "react-native-collapsible": "^1.4.0",
    "react-native-datepicker": "^1.7.2",
    "react-native-device-info": "^2.2.2",
    "react-native-document-picker": "^3.2.2",
    "react-native-dotenv": "^0.2.0",
    "react-native-fast-image": "6.1.1",
    "react-native-firebase": "^5.5.5",
    "react-native-floating-labels": "^1.1.9",
    "react-native-fs": "^2.13.3",
    "react-native-geolocation-service": "^3.1.0",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-global-font": "^1.0.2",
    "react-native-image-picker": "0.28.0",
    "react-native-image-progress": "^1.1.1",
    "react-native-image-zoom-viewer": "^2.2.26",
    "react-native-keyboard-aware-scroll-view": "^0.8.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-maps": "^0.25.0",
    "react-native-permissions": "^1.2.0",
    "react-native-phone-input": "^0.2.4",
    "react-native-pose": "^0.9.1",
    "react-native-progress": "^3.6.0",
    "react-native-svg": "^9.5.3",
    "react-native-swipe-gestures": "^1.0.3",
    "react-native-switch-toggle": "^1.1.0",
    "react-native-vector-icons": "^6.6.0",
    "react-native-video": "^5.1.0-alpha1",
    "react-native-video-player": "^0.10.0",
    "react-native-zoom-lightbox": "0.0.2",
    "react-navigation": "^3.11.1",
    "react-navigation-redux-helpers": "^3.0.2",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "redux-connect-decorator": "^0.2.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "rn-round-checkbox": "^0.3.4",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/runtime": "^7.5.5",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react-native": "^4.0.1",
    "eslint": "^5.14.1",
    "eslint-config-react-app": "^3.0.7",
    "eslint-plugin-flowtype": "^3.12.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-react-native": "^3.6.0",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "jetifier": "^1.6.3",
    "lint-staged": "^8.1.4",
    "metro-react-native-babel-preset": "^0.55.0",
    "prettier": "^1.16.4",
    "react-test-renderer": "16.8.3",
    "remote-redux-devtools": "^0.5.16"
  },
  "jest": {
    "preset": "react-native"
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "eslint --fix --max-warnings 0",
      "git add"
    ]
  }
}
